@using QLNhaSachFahasa.Resources
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutHome.cshtml";
}
@section title{
    @ViewText.TITLE_PHAN_QUYEN
}


<div class="box orange"></div>
<div class="box grey"></div>
<div class="box green"></div>
<style>
    body {
        margin: 10px;
    }

.box {
        display: block;
    }
</style>
<script>
    gsap.to("h2.title", { duration: 1, opacity: 0.3 });
    gsap.to(".box", { duration: 2, x: 300 });
    gsap.to(".green", { duration: 3, rotation: 360, scale: 0.5 });
</script>
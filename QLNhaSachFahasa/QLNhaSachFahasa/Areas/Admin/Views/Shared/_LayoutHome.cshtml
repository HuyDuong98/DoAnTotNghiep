@using QLNhaSachFahasa.Resources
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>@ViewBag.Title</title>
    <link rel=”stylesheet” href=”https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css” />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    <link href="https://fonts.googleapis.com/css?family=Quicksand:500&display=swap" rel="stylesheet">
    <!-- Material Kit CSS -->

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="https://kendo.cdn.telerik.com/2020.1.406/styles/kendo.bootstrap-v4.min.css" />
    <link rel="stylesheet" href="https://kendo.cdn.telerik.com/2020.1.406/styles/kendo.mobile.all.min.css" />
    <link href="~/Content/Custom-kendo.css" rel="stylesheet" />
    <link href="~/Content/main.css" rel="stylesheet" />
    <link href="~/Content/custom/HomePage.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-4.4.1/css/bootstrap.min.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")
    <script src="https://code.jquery.com/jquery-1.12.3.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2020.1.406/js/angular.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2020.1.406/js/jszip.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2020.1.406/js/kendo.all.min.js"></script>
</head>
<body>
    <div class="site-wrapper">
        <div class="site-header d-lg-block">
            <div class="header-middle pt-10 pb-10">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-lg-6">
                            <a href="@Url.Action("Index","AdHome")" class="site-brand">
                                <img src="~/Content/images/icon/logoFahasa.png" />
                            </a>
                        </div>
                        @*<div class="col-lg-3">
                            <div class="header-phone">
                                <div class="icon">
                                    <i class="fa fa-phone" aria-hidden="true"></i>
                                </div>
                                <div class="text">
                                    <p> @ViewText.TITLE_FREE_SUPPORT</p>
                                    <p class="font-weight-bold number">@ViewText.TITLE_SDT_CONTACT</p>
                                </div>
                            </div>
                        </div>*@
                        <div class="col-lg-6">
                            <div class="main-navigation flex-lg-right">
                                <ul class="main-menu menu-right ">
                                    <li class="menu-item has-children">
                                        <a href="@Url.Action("Index","Emloyee")">@ViewText.TITLE_EMPLOYEE</a>
                                        <ul class="sub-menu">
                                            <li><a href="#">Danh sách nhân viên</a></li>
                                            <li><a href="#">Tạo mới nhân viên</a></li>
                                        </ul>

                                    </li>
                                    <li class="menu-item has-children mega-menu">
                                        <a href="javascript:void(0)">Sách <i class="fa fa-angle-down" aria-hidden="true"></i></a>
                                    </li>
                                    <li class="menu-item has-children">
                                        <a href="javascript:void(0)">Văn phòng phẩm <i class="fa fa-angle-down" aria-hidden="true"></i></a>

                                    </li>
                                    <li class="menu-item">
                                        <a href="contact.html">Liên hệ</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="mb-5">
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>